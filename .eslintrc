{
  "extends": ["react-app", "react-app/jest"],
  "rules": {
    "curly": ["error", "all"],
    "max-lines-per-function": ["error", 300],
    "max-params": ["error", 4],
    // "no-console": ["error", { "allow": ["info", "error"] }], // TODO only needed on build
    "testing-library/no-container": "off",
    "testing-library/no-node-access": "off"
  }
}
