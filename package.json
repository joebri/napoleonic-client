{
  "name": "napoleonic-client",
  "version": "1.0.0",
  "description": "Client for Napoleonic Uniformology",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.7.0",
    "@cloudinary/react": "^1.8.1",
    "@cloudinary/url-gen": "^1.8.6",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.10.9",
    "@mui/material": "^5.10.9",
    "@mui/system": "^5.10.9",
    "cloudinary": "^1.33.0",
    "cra-template-typescript": "1.2.0",
    "graphql": "^16.6.0",
    "history": "^5.3.0",
    "husky": "^8.0.3",
    "lodash.debounce": "^4.0.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-router-dom": "^6.4.2",
    "react-scripts": "5.0.1",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4"
  },
  "scripts": {
    "start:dev": "react-scripts start",
    "build": "react-scripts build",
    "test:dev": "react-scripts test",
    "test": "jest",
    "lint": "eslint src/**/*.ts src/**/*.tsx --max-warnings=0",
    "prepare": "husky install",
    "docker:serve": "serve -s /app",
    "docker:up": "docker-compose --file docker-compose.prod.yml up --detach",
    "docker:down": "docker-compose --file docker-compose.prod.yml down"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "npm run lint"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.1.2",
    "@types/lodash": "^4.14.186",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/react-router-dom": "^5.3.3",
    "jest-environment-jsdom": "^29.3.1",
    "jest-extended": "^3.2.3",
    "jest-html-reporter": "^3.7.0",
    "lint-staged": "^13.1.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "web-vitals": "^3.0.3"
